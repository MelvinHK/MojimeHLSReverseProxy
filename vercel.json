{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)", 
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://mojime.vercel.app"
        }
      ]
    }
  ]
}